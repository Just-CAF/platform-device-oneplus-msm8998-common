get_prop(priv_app, camera_prop)
allow priv_app device:dir { open read };
allow priv_app proc_interrupts:file { open read };
allow priv_app proc_modules:file { getattr open read };
allow priv_app firmware_file:file getattr;
allow priv_app firmware_file:filesystem getattr;
allow priv_app firmware_file:dir { getattr search };
allow priv_app bt_firmware_file:filesystem { getattr };
allow priv_app sysfs_loop:file { read open };
allow priv_app sysfs:dir read;

dontaudit priv_app mnt_vendor_file:dir search;
